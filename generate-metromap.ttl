sam:BB
  spin:rule [
      a sp:Construct ;
      sp:templates (
          [
            sp:object <http://metro.politie.nl#S> ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "i" ;
              ] ;
          ]
          [
            sp:object <http://metro.politie.nl#L> ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "sbb" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "i" ;
              ] ;
            sp:predicate <http://metro.politie.nl#T> ;
            sp:subject [
                sp:varName "sbb" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "sbb" ;
              ] ;
            sp:predicate <http://metro.politie.nl#T> ;
            sp:subject [
                sp:varName "i2" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "color" ;
              ] ;
            sp:predicate <http://metro.politie.nl#color> ;
            sp:subject [
                sp:varName "sbb" ;
              ] ;
          ]
        ) ;
      sp:where (
          [
            sp:object [
                sp:varName "abb" ;
              ] ;
            sp:predicate sam:R ;
            sp:subject [
                sp:varName "sbb" ;
              ] ;
          ]
          [
            sp:object sam:ABB ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "abb" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "l" ;
              ] ;
            sp:predicate rdfs:label ;
            sp:subject [
                sp:varName "sbb" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "i" ;
              ] ;
            sp:predicate sam:A ;
            sp:subject [
                sp:varName "abb" ;
              ] ;
          ]
          [
            sp:object sam:I ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "i" ;
              ] ;
          ]
          [
            a sp:Bind ;
            sp:expression [
                a functions:getColor ;
                sp:arg1 [
                    sp:varName "l" ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "color" ;
              ] ;
          ]
          [
            a sp:Optional ;
            sp:elements (
                [
                  sp:object [
                      sp:varName "i" ;
                    ] ;
                  sp:predicate sam:D ;
                  sp:subject [
                      sp:varName "i2" ;
                    ] ;
                ]
              ) ;
          ]
        ) ;
      rdfs:comment "00003 metromap mapping" ;
    ] ;
.
sam:C
